
@{
    ViewBag.Title = "ProductAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="~/Content/Admin.css" rel="stylesheet" type="text/css" />
<link href="~/Content/ProductAdmin.css" rel="stylesheet" />
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

<div class="layout__content">
    <div class="layout__content-main">
       <span style="display: flex; padding: 1rem; font-size: 20px; font-weight: 600; color: #6b6b6b;">Products</span>
           <div class="admin-product-list">
              <table >
                  <tr >
                     <th></th>
                     <th>Image</th>
                      <th>Name</th>
                      <th>Price</th>
                      <th>Type</th>
                      </tr>
                  <tr>
                      <td>1</td>
                      <td>
                      <img src="https://cdn.cellphones.com.vn/media/catalog/product/cache/7/small_image/220x175/9df78eab33525d08d6e5fb8d27136e95/i/p/iphone-12_7__6.jpg"></img>
                      </td>
                      <td>Iphone 12</td>
                      <td>20</td>
                      <td>Iphone</td>
                      <td class="delete-product">
                          @*<DeleteOutlined />*@
                          <span role="img" aria-label="delete" class="anticon anticon-delete"><svg style="margin-left:12px" viewBox="64 64 896 896" focusable="false" data-icon="delete" width="1em" height="1em" fill="currentColor" aria-hidden="true"><path d="M360 184h-8c4.4 0 8-3.6 8-8v8h304v-8c0 4.4 3.6 8 8 8h-8v72h72v-80c0-35.3-28.7-64-64-64H352c-35.3 0-64 28.7-64 64v80h72v-72zm504 72H160c-17.7 0-32 14.3-32 32v32c0 4.4 3.6 8 8 8h60.4l24.7 523c1.6 34.1 29.8 61 63.9 61h454c34.2 0 62.3-26.8 63.9-61l24.7-523H888c4.4 0 8-3.6 8-8v-32c0-17.7-14.3-32-32-32zM731.3 840H292.7l-24.2-512h487l-24.2 512z"></path></svg></span>
                      </td>
                      <td class="update-product">
                          <Link>
                          @*<EditOutlined></EditOutlined>*@
                          <span role="img" aria-label="edit" class="anticon anticon-edit"><svg style="margin-left:12px" viewBox="64 64 896 896" focusable="false" data-icon="edit" width="1em" height="1em" fill="currentColor" aria-hidden="true"><path d="M257.7 752c2 0 4-.2 6-.5L431.9 722c2-.4 3.9-1.3 5.3-2.8l423.9-423.9a9.96 9.96 0 000-14.1L694.9 114.9c-1.9-1.9-4.4-2.9-7.1-2.9s-5.2 1-7.1 2.9L256.8 538.8c-1.5 1.5-2.4 3.3-2.8 5.3l-29.5 168.2a33.5 33.5 0 009.4 29.8c6.6 6.4 14.9 9.9 23.8 9.9zm67.4-174.4L687.8 215l73.3 73.3-362.7 362.6-88.9 15.7 15.6-89zM880 836H144c-17.7 0-32 14.3-32 32v36c0 4.4 3.6 8 8 8h784c4.4 0 8-3.6 8-8v-36c0-17.7-14.3-32-32-32z"></path></svg></span>
                        
                          </Link>
                      </td>
                                   @* <td class="review-product">
                                        <Link>
                                        <FormOutlined></FormOutlined>
                                        </Link>
                                    </td>*@
                                </tr>
                            </table>

                            @*<div class="pagination">
                            <Pagination defaultCurrent={1} current={currentPage} total={pages*10} onChange={HandleChangePage} />
                        </div>*@

           </div>
    </div>
</div>

